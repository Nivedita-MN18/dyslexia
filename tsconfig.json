{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "baseUrl": "./"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js", // Include .js files if you have any
